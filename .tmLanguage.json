{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "VNScript",
    "patterns": [
      {
        "include": "#keywords"
      },
      {
        "include": "#strings"
      },
      {
        "include": "#numbers"
      }
    ],
    "repository": {
      "keywords": {
        "match": "\\b(label|text|say|choice|jump|character|exp|bg|sound|after|start-dialogue|end-dialogue)\\b",
        "name": "keyword.control.vnscript"
      },
      "strings": {
        "begin": "\"",
        "end": "\"",
        "name": "string.quoted.double.vnscript"
      },
      "numbers": {
        "match": "\\b\\d+\\b",
        "name": "constant.numeric.vnscript"
      }
    },
    "scopeName": "source.vnscript",
    "uuid": "45ed6307-ee60-4453-ab94-1a88c7eef326"
}  